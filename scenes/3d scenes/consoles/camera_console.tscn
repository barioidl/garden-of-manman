script/source = "extends Control

var root:Node3D
@export var head:Node3D
@export var noise_sfx:AudioList
@export var truth_sfx:AudioList

func _enter_tree() -> void:
	root = get_parent().root

func _ready() -> void:
	connect_buttons()

func connect_buttons():
	$\"door\".connect('pressed',toggle_door)
	$\"light\".connect('pressed',toggle_light)
	$\"noise\".connect('pressed',play_loud_noise)
	
	var final_button := $\"confess\"
	if randi_range(0,10) > 1:
		final_button.visible = false
	else:
		final_button.visible = true
		final_button.connect('pressed',confess)

func toggle_door():
	var pos = head.get_contact()
	var lock := ProximityTool.get_closest_node3d(NL.locks, pos)
	if lock == null:	return
	lock.lock_toggle()

func toggle_light():
	var pos = head.get_contact()
	var lock := ProximityTool.get_closest_node3d(NL.light_buttons, pos)
	if lock == null:	return
	lock.toggle()

func play_loud_noise():
	var pos = head.get_contact()
	AudioPool.create_sound_3d(pos, noise_sfx)

func confess():
	var pos = head.global_position
	var speaker := AudioPool.create_sound_3d(pos, truth_sfx)
	if speaker == null:
		return
	speaker.connect('finished', on_confessed)

func on_confessed():
	pass

func _print(line):
	return
	print(line)
"

