; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Garden of Manman"
run/main_scene="res://scenes/3d scenes/test map.tscn"
config/features=PackedStringArray("4.0", "Mobile")
run/max_fps=60
boot_splash/bg_color=Color(0.968627, 0.968627, 0.968627, 1)
boot_splash/image="res://made with punity.png"
boot_splash/use_filter=false
config/icon="res://icon.svg"
boot_splash/minimum_display_time=1

[audio]

buses/default_bus_layout="res://data/audio/new_audio_bus_layout.tres"
driver/driver=""

[autoload]

AudioPool="*res://scripts/audio/audio_pool.gd"
NavAgentPool="*res://scripts/nav_agent/nav_agent_pool.gd"
Curves="*res://data/curves.tscn"
Goap="*res://scripts/GOAP/implementation/goap.gd"
WorldState="*res://scripts/GOAP/world state.gd"
ProximityTool="*res://scripts/proximity tool.gd"
Interface="*res://scripts/interface_accessor.gd"
PerformanceCap="*res://scripts/performance cap.gd"
PlayerInputs="*res://scenes/3d scenes/player inputs.tscn"
Hud="*res://scenes/2d scenes/hud.tscn"
NL="*res://scripts/name list.gd"

[debug]

gdscript/warnings/enable=false
gdscript/warnings/unsafe_property_access=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/aspect="keep_width"
window/vsync/vsync_mode=2

[editor]

movie_writer/movie_file="F:/footage/godot.avi"
movie_writer/fps=30
naming/node_name_num_separator=2
script/templates_search_path="res://scripts/script_templates"
naming/default_signal_callback_name="_on_{signal_name}"

[filesystem]

import/blender/enabled=false

[importer_defaults]

wavefront_obj={
"generate_tangents": true,
"offset_mesh": Vector3(0, 0, 0),
"optimize_mesh": true,
"scale_mesh": Vector3(1, 1, 1)
}
texture={
"compress/channel_pack": 1,
"compress/hdr_compression": 2,
"compress/high_quality": false,
"compress/lossy_quality": 1.0,
"compress/mode": 2,
"compress/normal_map": 2,
"detect_3d/compress_to": 1,
"mipmaps/generate": false,
"mipmaps/limit": -1,
"process/fix_alpha_border": true,
"process/hdr_as_srgb": false,
"process/hdr_clamp_exposure": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"process/size_limit": 320,
"roughness/mode": 0,
"roughness/src_normal": ""
}

